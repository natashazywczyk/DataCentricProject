<header>
    <h1>G00415696</h1>
</header>
<style>
     table, th, td {
        border: 1px solid black; /* Adds black borders to the table, header, and cells */
    }
</style>

<body>
    <table>
        <tr>
            <th>ID</th>
            <th>Module</th>
            <th>Grade</th>
        </tr>

        <% gradesList.forEach((grade) => { %>
            <tr>
                <td><%= grade.StudentName %></td>
                <td><%= grade.ModuleName %></td>
                <td><%= grade.Grade %></td> <!--Make sure after . is what the variable is called within data array, initialised in function in mySqlD.js-->
            </tr>
        <% }) %>
    </table>

    <br>
    <a href="/">Home</a>
</body>